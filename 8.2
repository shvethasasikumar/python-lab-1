t = input()
k = int(input())
a = t.split(",")
l = [int(x) for x in a]
count = 0
x = set()

for i in range(len(l)):
    for j in range(i + 1, len(l)):
        if l[i] + l[j] == k:
            s = (l[i], l[j])
            if s not in x and (l[j], l[i]) not in x:
                count += 1
                x.add(s)

print(count)
